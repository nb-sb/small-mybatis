<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "https://github.com/nb-sb/small-mybatis/">

<mapper namespace="com.nbsb.mybatis.test.dao.IHomeInfoDao">

    <select id="queryHomeName" parameterType="java.lang.Long" resultType="com.nbsb.mybatis.test.po.Home">
        SELECT id,name
        FROM home
        where id = #{id}
    </select>


</mapper>
